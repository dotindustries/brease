{
  "name": "@brease/core",
  "description": "The framework agnostic core that powers Brease",
  "version": "0.0.1",
  "author": "nadilas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotindustries/brease",
    "directory": "packages/core"
  },
  "homepage": "https://brease.run",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/dotindustries"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./src": {
      "default": "./src/index.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --config tsup.config.ts --watch",
    "build": "tsup src/index.ts --config tsup.config.ts",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@brease/sdk": "^0.0.3",
    "cachified": "^3.5.4",
    "lodash-es": "^4.17.21",
    "lru-cache": "^10.0.0",
    "object-hash": "^3.0.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/object-hash": "^3.0.2",
    "tsup": "6.7.0"
  }
}
