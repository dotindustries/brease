{
  "name": "@brease/react-brease-devtools",
  "version": "0.0.1",
  "description": "Developer tools to interact with and visualize the brease/react",
  "author": "nadilas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotindustries/brease",
    "directory": "packages/react-brease-devtools"
  },
  "homepage": "https://brease.run",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/dotindustries"
  },
  "types": "build/lib/index.d.ts",
  "main": "build/lib/index.js",
  "module": "build/lib/index.esm.js",
  "exports": {
    ".": {
      "types": "./build/lib/index.d.ts",
      "import": "./build/lib/index.mjs",
      "default": "./build/lib/index.js"
    },
    "./production": {
      "types": "./build/lib/index.d.ts",
      "import": "./build/lib/index.prod.mjs",
      "default": "./build/lib/index.prod.js"
    },
    "./build/lib/index.prod.js": {
      "types": "./build/lib/index.d.ts",
      "import": "./build/lib/index.prod.mjs",
      "default": "./build/lib/index.prod.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "build/lib/*",
    "build/umd/*",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./build",
    "test_eslint": "eslint --ext .ts,.tsx ./src",
    "test_types": "tsc",
    "test_lib": "jest --config ./jest.config.ts",
    "test_lib_dev": "pnpm run test:lib --watch"
  },
  "devDependencies": {
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/use-sync-external-store": "^0.0.3",
    "react": "^18.2.0",
    "react-17": "npm:react@^17.0.2",
    "react-dom": "^18.2.0",
    "react-dom-17": "npm:react-dom@^17.0.2",
    "react-error-boundary": "^4.0.4",
    "@brease/react": "workspace:*"
  },
  "dependencies": {
    "@tanstack/match-sorter-utils": "^8.8.4",
    "superjson": "^1.12.3",
    "use-sync-external-store": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "@brease/react": "workspace:*"
  }
}
